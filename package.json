{
  "name": "fetch-opts",
  "version": "1.0.0",
  "description": "FetchOptionsBuilder is a fluent and easy-to-use utility to build fetch options for making HTTP requests with the Fetch API. It provides a convenient way to configure common options such as Bearer token, method, content type, body, credentials, and custom headers.",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/cjs/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js"
    },
    "./package.json": "./package.json"
  },
  "scripts": {
    "build": "tsc && tsc -p tsconfig.esm.json",
    "prepublishOnly": "npm run build",
    "test": "jest",
    "prepare": "husky install"
  },
  "keywords": [
    "fetch",
    "options",
    "builder",
    "http",
    "request",
    "fetch-api",
    "fetch-options",
    "fetch-options-builder",
    "bearer-token",
    "content-type",
    "headers"
  ],
  "author": "Bocar Wane",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^29.5.1",
    "husky": "^8.0.3",
    "jest": "^29.5.0",
    "prettier": "^2.8.8",
    "ts-jest": "^29.1.0",
    "typescript": "^5.0.4"
  }
}
